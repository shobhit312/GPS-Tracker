<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="oval">
            <solid
                android:color="#ffaeef2d" />
            <stroke
                android:width="1dp"
                android:color="#ff669937" />
            <corners
                android:radius="3dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <gradient
                android:startColor="#ffabef0f"
                android:endColor="#ff46990d"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ff7f9908" />
            <corners
                android:radius="4dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>